function destroyGameplayObjects() {
	with (Sentry)
	    instance_destroy();
	with (SentryGibs)
	    instance_destroy();
	with (Rocket)
	    instance_destroy();
	with (Shot)
	    instance_destroy();
	with (BurningProjectile)
	    instance_destroy();
	with (Needle)
	    instance_destroy();



}
