function resetRoundStats() {
	with (Player)
	{
	    roundStats[KILLS] = 0;
	    roundStats[DEATHS] = 0;
	    roundStats[CAPS] = 0;
	    roundStats[ASSISTS] = 0;
	    roundStats[DESTRUCTION] = 0;
	    roundStats[STABS] = 0;
	    roundStats[HEALING] = 0;
	    roundStats[DEFENSES] = 0;
	    roundStats[INVULNS] = 0;
	    roundStats[BONUS] = 0;
	    roundStats[DOMINATIONS] = 0;
	    roundStats[REVENGE] = 0;
	    roundStats[POINTS] = 0;
	}



}
