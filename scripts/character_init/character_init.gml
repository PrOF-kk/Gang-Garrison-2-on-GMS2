function character_init() {
	// Initialize character map
	global.characterMap = ds_map_create();

	ds_map_add(global.characterMap, CLASS_SCOUT, Scout);
	ds_map_add(global.characterMap, CLASS_PYRO, Pyro);
	ds_map_add(global.characterMap, CLASS_SOLDIER, Soldier);
	ds_map_add(global.characterMap, CLASS_DEMOMAN, Demoman);
	ds_map_add(global.characterMap, CLASS_HEAVY, Heavy);
	ds_map_add(global.characterMap, CLASS_ENGINEER, Engineer);
	ds_map_add(global.characterMap, CLASS_MEDIC, Medic);
	ds_map_add(global.characterMap, CLASS_SPY, Spy);
	ds_map_add(global.characterMap, CLASS_SNIPER, Sniper);
	ds_map_add(global.characterMap, CLASS_QUOTE, Quote);



}
